{
    "extends": ["eslint:recommended"],
    "parserOptions": {
        "ecmaVersion": 2016
    },
    "globals": {
        "require": true,
        "Promise": true
    },
    "env": {
        "es6": true,
        "node": true
    },
    "rules": {
        "arrow-parens": "warn",
        "no-console": "off",
        "semi": "error"
    }
}